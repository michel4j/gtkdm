<?xml version="1.0" encoding="UTF-8"?>
<glade-catalog name="gtkplugin" library="gladepython" depends="gtk+" domain="glade-3">
  <init-function>glade_python_init</init-function>

  <glade-widget-classes>
    <glade-widget-class name="Display" generic-name="display" title="Display" icon-name="widget-gtk-window"/>
    <glade-widget-class name="TextMonitor" generic-name="textmonitor" title="TextMonitor" icon-name="widget-gtk-label"/>
    <glade-widget-class name="LineMonitor" generic-name="linemonitor" title="LineMonitor" icon-name="widget-gtk-separator"/>
    <glade-widget-class name="Byte" generic-name="byte" title="Byte" icon-name="widget-gtk-stacksidebar"/>
    <glade-widget-class name="Indicator" generic-name="indicator" title="Indicator" icon-name="widget-gtk-eventbox"/>
    <glade-widget-class name="ScaleControl" generic-name="scalecontrol" title="ScaleControl" icon-name="widget-gtk-scale"/>
    <glade-widget-class name="TextControl" generic-name="textcontrol" title="TextControl" icon-name="widget-gtk-entry"/>
    <glade-widget-class name="TextLabel" generic-name="textlabel" title="Textlabel" icon-name="widget-gtk-label"/>
    <glade-widget-class name="CommandButton" generic-name="commandbutton" title="CommandButton" icon-name="widget-gtk-button"/>
    <glade-widget-class name="ChoiceButton" generic-name="choicebutton" title="ChoiceButton" icon-name="widget-gtk-stackswitcher"/>
    <glade-widget-class name="Gauge" generic-name="gauge" title="Gauge" icon-name="widget-gtk-frame"/>
    <glade-widget-class name="Symbol" generic-name="symbol" title="Symbol" icon-name="widget-gtk-symbol"/>

  </glade-widget-classes>
  
  <glade-widget-group name="gtkdm" title="GtkDM">
    <glade-widget-class-ref name="Display"/>
    <glade-widget-class-ref name="TextMonitor"/>
    <glade-widget-class-ref name="LineMonitor"/>
    <glade-widget-class-ref name="Byte"/>
    <glade-widget-class-ref name="Indicator"/>
    <glade-widget-class-ref name="ScaleControl"/>
    <glade-widget-class-ref name="TextControl"/>
    <glade-widget-class-ref name="TextLabel"/>
    <glade-widget-class-ref name="CommandButton"/>
    <glade-widget-class-ref name="ChoiceButton"/>
    <glade-widget-class-ref name="Gauge"/>
    <glade-widget-class-ref name="Symbol"/>
  </glade-widget-group>

</glade-catalog>
